{
  "name": "@tdukart/nextui-blurhash-image",
  "version": "2.0.21",
  "description": "An extension of the NextUI Image component that allows you to use Blurhash to display a placeholder while the image is loading.",
  "keywords": [
    "image"
  ],
  "author": "Todd Dukart <tdukart@mechninja.com>",
  "contributors": [
    {
      "name": "Junior Garcia (created base package)",
      "email": "jrgarciadev@gmail.com"
    }
  ],
  "homepage": "https://nextui.org",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdukart/nextui.git",
    "directory": "packages/components/blurhash-image"
  },
  "bugs": {
    "url": "https://github.com/tdukart/nextui/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@nextui-org/system": "workspace:*",
    "@nextui-org/theme": "workspace:*",
    "@nextui-org/shared-utils": "workspace:*",
    "@nextui-org/react-utils": "workspace:*",
    "@nextui-org/use-image": "workspace:*",
    "react-blurhash": "^0.3.0"
  },
  "devDependencies": {
    "clean-package": "2.2.0",
    "react": "^18.0.0"
  },
  "clean-package": "../../../clean-package.config.json"
}
